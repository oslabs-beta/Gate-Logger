{
    "name": "gate-logger",
    "version": "1.0.0",
    "description": "A backend library to log GraphQL query data to  Gateway developer portal",
    "main": "./dist/src/index.js",
    "types": "./dist/src/index.d.ts",
    "scripts": {
        "test": "jest -c ./jest.unit.json",
        "test:coverage": "jest --coverage -c ./jest.unit.json",
        "lint": "eslint src test",
        "lint:fix": "eslint --fix src test",
        "prettier": "prettier --write .",
        "prepare": "husky install",
        "test:server": "nodemon --exec ts-node --files ./test/server.ts",
        "build": "tsc",
        "build:fix": "node node_modules/.bin/yab dist"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/oslabs-beta/gate-logger.git"
    },
    "author": "Evan McNeely, Stephan Halarewicz, Flora Yufei Wu, Jon Dewey, Milos Popovic",
    "license": "ISC",
    "keywords": [
        "graphql",
        "graphqlgate",
        "rate-limiting",
        "throttling",
        "query",
        "express",
        "logger"
    ],
    "bugs": {
        "url": "https://github.com/oslabs-beta/gate-logger/issues"
    },
    "homepage": "https://github.com/oslabs-beta/gate-logger#readme",
    "dependencies": {
        "axios": "^0.27.2",
        "body-parser": "^1.20.0",
        "express": "^4.18.1",
        "graphql": "^16.5.0",
        "mongoose": "^6.3.5"
    },
    "devDependencies": {
        "@types/express": "^4.17.13",
        "@types/jest": "^27.5.2",
        "@types/node": "^17.0.34",
        "@types/supertest": "^2.0.12",
        "@typescript-eslint/eslint-plugin": "^5.23.0",
        "@typescript-eslint/parser": "^5.23.0",
        "add-js-extension": "^1.0.4",
        "eslint": "^8.15.0",
        "eslint-config-airbnb-base": "^15.0.0",
        "eslint-config-airbnb-typescript": "^17.0.0",
        "eslint-config-prettier": "^8.5.0",
        "eslint-plugin-import": "^2.26.0",
        "eslint-plugin-prettier": "^4.0.0",
        "husky": "^8.0.1",
        "jest": "^28.1.0",
        "lint-staged": "^12.4.1",
        "msw": "^0.42.3",
        "prettier": "2.6.2",
        "supertest": "^6.2.3",
        "ts-jest": "^28.0.3",
        "ts-node": "^10.8.0",
        "typescript": "^4.6.4"
    },
    "lint-staged": {
        "*.{js, ts}": "eslint --cache --fix",
        "*.{js,ts,css,md}": "prettier --write --ignore-unknown"
    }
}
